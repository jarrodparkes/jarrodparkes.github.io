---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <image>
        <url>http://jarrodparkes.github.io/images/fork.png</url>
        <title>Jarrod Parkes</title>
        <link>http://jarrodparkes.github.io</link>
        <width>64</width>
        <height>64</height>
        <description>Learner. Teacher. Developer.</description>
    </image>
    <title>{{ site.title | xml_escape }}</title>
    <description>{% if site.description %}{{ site.description | xml_escape }}{% endif %}</description>
    <link>
    {{ site.url }}</link>
    {% for post in site.posts %}
      {% unless post.link %}
      <item>
            <title>{{ post_title | xml_escape }}</title>
            <description>{{ post.content | xml_escape }}</description>
            <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
            <link>{{ post.url | prepend: site.site_url }}</link>
            <guid isPermaLink="true">{{ post.url | prepend: site.site_url }}</guid>
            {% for tag in post.tags %}
            <category>{{ tag | xml_escape }}</category>
            {% endfor %}
            {% for cat in post.categories %}
            <category>{{ cat | xml_escape }}</category>
            {% endfor %}
        </item>
      {% endunless %}
    {% endfor %}
  </channel>
</rss>